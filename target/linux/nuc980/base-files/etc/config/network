config interface 'loopback'
	option proto 'static'
	option netmask '255.0.0.0'
	option device 'lo'

config interface 'eth0'
	option proto 'dhcp'
	option device 'eth0'

config interface 'eth1'
	option proto 'static'
	option device 'eth1'
	option ipaddr '172.16.0.2'
	option netmask '255.255.255.0'

config interface '3g'
	option proto '3g'
	option device '/dev/ttyUSB3'
	option service 'umts'
	option ipv6 'auto'
